buildscript {
    repositories {
        mavenLocal()
        maven { url "https://dl.bintray.com/soywiz/soywiz" }
        maven { url  "https://plugins.gradle.org/m2/" }
        mavenCentral()
    }
    dependencies {
        classpath("com.soywiz:korge-gradle-plugin:1.2.0")
    }
}

apply(plugin = "korge")